import POM.BaseTest;
import POM.LoginPage;

public class MainTest extends BaseTest{

	@BeforeSuite
	public void setup()
	{
		launchURL("chrome");
	}

	@AfterSuite
	public void teardown()
	{
		driver.quit();
	}
	
		@Test
		public void ValidLogin()
		{
			LoginPage lp = new LoginPage(driver);
			lp.login();
		}
		

	

}
